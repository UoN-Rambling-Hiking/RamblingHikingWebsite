BEGIN:VCALENDAR
VERSION:2.0
PRODID:-//ramsoc.co.uk//Ramsoc Calendar 1.0//EN
METHOD:PUBLISH
LAST-MODIFIED:{{ now.Format "20060102T150405Z" }}
CALSCALE:GREGORIAN{{ range .RegularPages }}{{ if isset .Params "date" }}
BEGIN:VEVENT
SUMMARY:{{ .Title }}
DESCRIPTION:More details can be found at {{ .Permalink }}
UID:{{replace .Title " " "-" }}@ramsoc.co.uk
SEQUENCE:0
STATUS:CONFIRMED
DTSTAMP:{{dateFormat "20060102T150405Z" .Date}}
DTSTART:{{ dateFormat "20060102T150405Z" .Date }}
{{ if isset .Params "end_date" -}}
{{- $d := .Params.end_date | time.AsTime -}}
DTEND:{{ dateFormat "20060102" $d }}T220000Z
{{- else -}}
DTEND:{{ dateFormat "20060102" .Date }}T183000Z
{{- end }}
LOCATION:{{ .Title }}
GEO:{{ .Params.lat }};{{ .Params.lng }}
END:VEVENT{{ end }} {{ end }}
END:VCALENDAR